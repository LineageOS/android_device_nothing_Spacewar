# Display
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display-primary               u:object_r:vendor_sysfs_graphics:s0

# Glyph LEDs
genfscon sysfs /devices/platform/soc/984000.i2c/i2c-0/0-0020/leds/aw210xx_led/single_led_br u:object_r:sysfs_leds:s0

# Media
genfscon sysfs /devices/platform/soc/aa00000.qcom,vidc/sku_version              u:object_r:vendor_sysfs_sku:s0

# Sensors
genfscon sysfs /devices/platform/soc/a94000.spi/spi_master/spi0/spi0.0/fts_gesture_fod_pressed          u:object_r:vendor_sysfs_sensors:s0
genfscon sysfs /devices/platform/soc/a94000.spi/spi_master/spi0/spi0.0/fts_gesture_single_tap_pressed   u:object_r:vendor_sysfs_sensors:s0

# PowerShare
genfscon sysfs /class/qcom-battery/wireless_boost_en                            u:object_r:sysfs_powershare:s0
genfscon sysfs /class/qcom-battery/wls_reverse_status                           u:object_r:sysfs_powershare:s0

# Wakeup
genfscon sysfs /devices/platform/soc/984000.i2c/i2c-0/0-0020/wakeup             u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/a84000.i2c/i2c-2/2-0028/wakeup             u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/a94000.spi/spi_master/spi0/spi0.0/wakeup   u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/a98000.spi/spi_master/spi1/spi1.0/wakeup   u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/c440000.qcom,spmi/spmi-0/spmi0-03/c440000.qcom,spmi:qcom,pm8350b@3:qcom,amoled/wakeup        u:object_r:sysfs_wakeup:s0
